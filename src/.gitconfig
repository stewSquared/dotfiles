[user]
	name = Stewart Stewart
	email = stewinsalot@gmail.com
[core]
	autocrlf = input
	safecrlf = true
	excludesfile = /home/stew/.gitignore_global
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  type = cat-file -t
  dump = cat-file -p
  hist = log --pretty=format:'%w(96,0,14)%h %ad | %s%d'\
             --date=format:"%d%b" --graph --abbrev=4
  hista = log --pretty=format:'%w(96,0,14)%h %ad | %s%d - %an'\
              --date=format:"%d%b" --graph --abbrev=4
[credential]
	helper = cache
[push]
	default = simple
